<h1>Dossier | All Posts</h1>

<div class="links">
  <%= link_to "New Post", new_post_path %>
</div>

<div class="allposts">
  <% @posts.each do |post| %>
    <p>
      <%= link_to post.title, post_path(post) %><br>
      <%= link_to image_tag(post.picture_url), post_path(post) %>
    </p>
  <% end %>
</div>
